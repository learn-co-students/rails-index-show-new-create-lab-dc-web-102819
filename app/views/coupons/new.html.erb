<!DOCTYPE html>
<html>
<head>
	<title>New Coupon</title>
</head>
<body>

	<h1>Coupon Form</h1>

	<%= form_for @coupon do |f| %>
		<div>
			<%= f.label 'store' %>
			<%= f.text_field 'store' %>
		</div>
		<div>
			<%= f.label 'coupon_code' %>
			<%= f.text_field 'coupon_code' %>
		</div>
		<div>
			<%= f.submit "Submit Coupon" %>
		</div>
	<% end %>
</body>
</html>
